<h2>Post List</h2>

<a href="/post/add_post"><button>Add New Post</button></a><br><br>
<table border>
	<tr>
		<th>Id</th>
		<th>Title</th>
		<th>Body</th>
		<th>Action</th>
	</tr>
	  <% for(var i=0; i<vdata.length; i++) {%>
	<tr>
		<td><%= vdata[i]._id %></td>
		<td><%= vdata[i].title %></td>
		<td><%= vdata[i].body %></td>
		<td></td>
	</tr>
	<% } %>

</table>